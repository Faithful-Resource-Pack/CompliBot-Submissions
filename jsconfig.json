{
	"exclude": ["node_modules", "json"],
	"compilerOptions": {
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"@functions/*": ["./functions/*"],
			"@helpers/*": ["./helpers/*"],
			"@commands/*": ["./commands/*"],
			"@events/*": ["./events/*"],
