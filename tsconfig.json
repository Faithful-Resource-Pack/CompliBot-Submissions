{
	"exclude": ["node_modules", "backups", "downloadedTextures", "instapassedTextures"],
	"compilerOptions": {
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"target": "ESNEXT",
		"noEmit": true,
		"paths": {
			"@functions/*": ["./src/functions/*"],
			"@helpers/*": ["./src/helpers/*"],
			"@commands/*": ["./src/commands/*"],
			"@events/*": ["./src/events/*"],
			"@submission/*": ["./src/functions/submission/*"],
			"@images/*": ["./src/functions/images/*"],
			"@interfaces/*": ["./src/interfaces/*"],
			"@resources/*": ["./resources/*"],
			"@index": ["./src/index.ts"],
		}
	}
}