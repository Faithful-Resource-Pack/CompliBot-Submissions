{
	"exclude": ["node_modules", "backups", "downloadedTextures", "instapassedTextures"],
	"compilerOptions": {
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "ESNEXT",
		"baseUrl": ".",
		"paths": {
			"@functions/*": ["./functions/*"],
			"@helpers/*": ["./helpers/*"],
			"@commands/*": ["./commands/*"],
			"@events/*": ["./events/*"],
			"@submission/*": ["./functions/submission/*"],
			"@images/*": ["./functions/images/*"],
			"@interfaces/*": ["./interfaces/*"],
			"@resources/*": ["./resources/*"],
			"@index": ["./index.ts"],
		}
	}
}