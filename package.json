{
    "name": "complibot-submissions",
    "version": "1.0.0",
    "description": "Fully automated texture pushing and submission system for the Faithful Discord servers.",
    "main": "index.js",
    "scripts": {
        "start": "node --use_strict --trace-warnings index.js",
        "dev": "nodemon --ignore resources/ --ignore backups/",
        "prettier": "prettier \"{*.js,!(node*)**/*.js}\" --config .prettierrc --write"
    },
    "devDependencies": {
        "nodemon": "^3.1.2",
        "prettier": "^3.3.0"
    },
    "dependencies": {
        "@napi-rs/canvas": "^0.1.52",
        "@octokit/rest": "^20.1.1",
        "axios": "^1.7.2",
        "cron": "^3.1.7",
        "discord.js": "^14.15.3",
        "dotenv": "^16.4.5",
        "gif-encoder-2": "^1.0.5",
        "globby": "^14.0.1",
        "module-alias": "^2.2.3"
    },
    "_moduleAliases": {
        "@functions": "./functions",
        "@helpers": "./helpers",
        "@commands": "./commands",
        "@events": "./events",
        "@submission": "./functions/submission",
        "@images": "./functions/images",
        "@resources": "./resources",
        "@index": "./index.js"
    }
}
